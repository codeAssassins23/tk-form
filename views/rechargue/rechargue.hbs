			var element = document.querySelector("#stepperHolding");

			// Initialize Stepper
			var stepperPayment = new KTStepper(element);

			const buttonsNext1 = document.querySelectorAll('.btn-next1');
			buttonsNext1.forEach(button => {
				button.addEventListener('click', function() {
					stepperPayment.goNext();
