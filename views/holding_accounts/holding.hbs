                        <div class="page-title d-flex flex-column justify-content-center gap-1 me-3">
                            <!--begin::Title-->
                            <h1 class="page-heading d-flex flex-column justify-content-center text-gray-900 fw-bold fs-3 m-0">Holding accounts</h1>
                            <!--end::Title-->
                        
                        </div>
                                            <!--begin::Actions-->
                        <div class="d-flex align-items-start gap-2 gap-lg-3">
                            <a href="#" class="btn btn-warning h-40px fs-5 fw-bold" data-bs-toggle="modal" data-bs-target="#kt_modal_create_campaign"><i class="ki-outline ki-send fs-0"></i>Send payment</a>
                        </div>
                        <!--end::Actions-->
                    </div>
                
                    <!--end::Page title-->
                </div>
                    <!--end::Toolbar wrapper-->
            </div>
